<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
    <meta charset="UTF-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel='stylesheet' href='https://cdnjs.cloudflare.com/ajax/libs/bulma/0.7.5/css/bulma.css' />
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css"/>
    <script type = "text/javascript">

        function Warn() {


            var lmbd = document.forms["fom"]["lmbd"].value;
            if(lmbd==""){
                alert("Please enter Lab Manager Birthdate");
                return false;
            }else{

                var sl = document.forms["fom"]["sl"].value;
                if(sl==""){
                    alert("Please enter Site Location");
                    return false;
                }else{

                    var smnum = document.forms["fom"]["smnum"].value;
                    if(smnum==""){
                        alert("Please enter Site Manager Phone Number");
                        return false;
                    }else{

                        var colname = document.forms["fom"]["colname"].value;
                        if(colname==""){
                            alert("Please enter COL Name");
                            return false;
                        }else{

                            var colbd = document.forms["fom"]["colbd"].value;
                            if(colbd==""){
                                alert("Please enter COL Birthdate");
                                return false;
                            }else{

                                var asname = document.forms["fom"]["asname"].value;
                                if(asname==""){
                                    alert("Please enter Area Supervisor Name");
                                    return false;
                                }else{
                                    return true;
                                }

                            }

                        }

                    }

                }

            }


        }
    </script>
</head>
<body>

<div class="field" >


    <form id="fom" method="POST" action="/form" onsubmit="return Warn()">

        <h1 class="title is-2" align="center">Store Validation</h1>

        <div class="box">

        <div class="field is-horizontal">
            <div class="field-label is-normal">
                Site Manager No.
            </div>
            <div class="field-body">
                <div class="field is-narrow">
                    <div class="control">
                        <div class="select is-rounded">
                            <select name="smno">
                                <option>1</option>
                                <option>2</option>
                                <option>3</option>
                                <option>4</option>
                                <option>5</option>
                                <option>6</option>
                                <option>7</option>
                                <option>8</option>
                                <option>9</option>
                            </select>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="field is-horizontal">
            <div class="field-label is-normal">
                HK Staff No.
            </div>
            <div class="field-body">
                <div class="field is-narrow">
                    <div class="control">
                        <div class="select is-rounded">
                            <select name="hkno">
                                <option>1</option>
                                <option>2</option>
                                <option>3</option>
                                <option>4</option>
                                <option>5</option>
                                <option>6</option>
                                <option>7</option>
                                <option>8</option>
                                <option>9</option>
                            </select>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="field is-horizontal">
            <div class="field-label is-normal">
                SG No.
            </div>
            <div class="field-body">
                <div class="field is-narrow">
                    <div class="control">
                        <div class="select is-rounded">
                            <select name="sgno">
                                <option>1</option>
                                <option>2</option>
                                <option>3</option>
                                <option>4</option>
                                <option>5</option>
                                <option>6</option>
                                <option>7</option>
                                <option>8</option>
                                <option>9</option>
                            </select>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="field is-horizontal">
            <div class="field-label is-normal">
                No. of Pantry Boys
            </div>
            <div class="field-body">
                <div class="field is-narrow">
                    <div class="control">
                        <div class="select is-rounded">
                            <select name="pbno">
                                <option>1</option>
                                <option>2</option>
                                <option>3</option>
                                <option>4</option>
                                <option>5</option>
                                <option>6</option>
                                <option>7</option>
                                <option>8</option>
                                <option>9</option>
                            </select>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="field is-horizontal">
            <div class="field-label is-normal">
                No. of Lab Attendants
            </div>
            <div class="field-body">
                <div class="field is-narrow">
                    <div class="control">
                        <div class="select is-rounded">
                            <select name="lano">
                                <option>1</option>
                                <option>2</option>
                                <option>3</option>
                                <option>4</option>
                                <option>5</option>
                                <option>6</option>
                                <option>7</option>
                                <option>8</option>
                                <option>9</option>
                            </select>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        </div>

        <div class="field is-horizontal">
            <div class="field-label is-normal">Lab Manager Birthdate</div>
            <div class="field-body">
                <div class="field is-expanded">
                    <div class="field has-addons">
                        <p class="control">
                            <input class="input" type="date" name="lmbd">
                        </p>
                    </div>
                </div>
            </div>
        </div>

        <div class="field is-horizontal">
            <div class="field-label is-normal">
                Site Location
            </div>
            <div class="field-body">
                <div class="field">
                    <div class="control">
                        <textarea class="textarea" name="sl" placeholder="Enter the location" rows="3"></textarea>
                    </div>
                </div>
            </div>
        </div>

        <div class="field is-horizontal">
            <div class="field-label is-normal">Site Manager</div>
            <div class="field-body">
                <div class="field is-expanded">
                    <div class="field has-addons">
                        <p class="control">
                            <a class="button is-static">
                                +91
                            </a>
                        </p>
                        <p class="control">
                            <input class="input" type="tel" placeholder="Phone Number" name="smnum">
                        </p>
                    </div>
                </div>
            </div>
        </div>


        <div class="field is-horizontal">
            <div class="field-label is-normal">
                COL Name
            </div>
            <div class="field-body">
                <div class="field">
                    <p class="control ">
                        <input class="input" name="colname" type="text" placeholder="Name">
                    </p>
                </div>
            </div>
        </div>

        <div class="field is-horizontal">
            <div class="field-label is-normal">COL Birthdate</div>
            <div class="field-body">
                <div class="field is-expanded">
                    <div class="field has-addons">
                        <p class="control">
                            <input class="input" name="colbd" type="date" name="stmgrnum">
                        </p>
                    </div>
                </div>
            </div>
        </div>


        <div class="field is-horizontal">
            <div class="field-label is-normal">
                Area Supervisor
            </div>
            <div class="field-body">
                <div class="field">
                    <p class="control ">
                        <input class="input" name="asname" type="text" placeholder="Name">
                    </p>
                </div>
            </div>
        </div>





        <div class="field is-horizontal">
            <div class="field-label">
            </div>
            <div class="field-body">
                <div class="field">
                    <div class="control">
                        <button class="button is-rounded is-primary" type="submit" value="SUBMIT">Submit</button>
                    </div>
                </div>
            </div>
        </div>



    </form>


</div>

</body>
</html>
